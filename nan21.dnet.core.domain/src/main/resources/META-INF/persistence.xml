<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="default"
		transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<!-- Dependencies -->
 		<class>net.nan21.dnet.core.domain.model.impldummy.AbstractSimpleEntityDummy</class>
 		<class>net.nan21.dnet.core.domain.model.impldummy.AbstractAuditableDummy</class>
		<class>net.nan21.dnet.core.domain.model.impldummy.AbstractTypeDummy</class>
		<class>net.nan21.dnet.core.domain.model.impldummy.AbstractTypeWithCodeDummy</class>
		 
		<class>net.nan21.dnet.core.domain.model.AbstractSimpleEntity</class>
		<class>net.nan21.dnet.core.domain.model.AbstractAuditable</class>
		<class>net.nan21.dnet.core.domain.model.AbstractType</class>
		<class>net.nan21.dnet.core.domain.model.AbstractTypeWithCode</class>
		
		<exclude-unlisted-classes>true</exclude-unlisted-classes> 		 
		<properties>
			<property name="eclipselink.weaving" value="static" />
		</properties>
	</persistence-unit>
</persistence>